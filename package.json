{
    "name": "naive-inertia-components",
    "version": "0.0.9",
    "description": "A Naive UI-to-Laravel Adaptor.",
    "main": "dist/mthlily-naive-inertia-js-components.ssr.js",
    "browser": "dist/mthlily-naive-inertia-js-components.esm.js",
    "module": "dist/mthlily-naive-inertia-js-components.esm.js",
    "unpkg": "dist/mthlily-naive-inertia-js-components.min.js",
    "types": "dist/types/Components/entry.esm.d.ts",
    "files": [
        "dist/*",
        "resources/js/Components/*"
    ],
    "sideEffects": false,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "prebuild": "rimraf ./dist",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
        "postbuild": "rimraf ./dist/types/dev ./dist/types/Components/entry.d.ts"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@inertiajs/progress": "^0.2.6",
        "@rollup/plugin-alias": "^3.1.2",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@tailwindcss/forms": "^0.4.0",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "@vue/babel-preset-app": "^4.5.15",
        "@vue/cli-plugin-babel": "^4.5.13",
        "@vue/cli-plugin-typescript": "^4.5.13",
        "@vue/cli-service": "^4.5.13",
        "@vue/compiler-sfc": "^3.0.11",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.2.4",
        "axios": "^0.21",
        "browserify-zlib": "^0.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.7.0",
        "eslint-plugin-vue": "^8.3.0",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "minimist": "^1.2.5",
        "postcss": "^8.2.13",
        "postcss-import": "^14.0.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.52.8",
        "rollup-plugin-multi-input": "^1.3.1",
        "rollup-plugin-node-externals": "^3.1.2",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "rollup-plugin-vue": "^6.0.0",
        "tailwindcss": "^3.0.0",
        "ts-loader": "^9.2.6",
        "ttypescript": "^1.5.12",
        "typescript": "^4.5.4",
        "vue-loader": "^16.1.2"
    },
    "peerDependencies": {
        "@inertiajs/inertia": "^0.10.0",
        "@inertiajs/inertia-vue3": "^0.5.1",
        "@types/ziggy-js": "^1.3.0",
        "naive-ui": "^2.24.1",
        "vue": "^3.0.5",
        "ziggy-js": "^1.4.2"
    }
}